{
  "name": "@asinino/node-red-kafkajs",
  "version": "0.0.6",
  "description": "A package to comunicate with a Kafka server and lets the user control the flow of message pipeline to control success and failure on message synchronization.",
  "homepage": "https://github.com/asinino/node-red-kafkajs#readme",
  "keywords": [
    "node-red",
    "kafka",
    "broker",
    "streaming",
    "consumer",
    "producer",
    "topic"
  ],
  "author": "Paulo Oliveira",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "npm pack --pack-destination tmp/",
    "start": "node node_modules/node-red/red.js -v -u . -s node-red-default-settings.js",
    "debug": "node --nolazy node_modules/node-red/red.js -v -u ./debug"
  },
  "node-red": {
    "version": ">=4.0.0",
    "nodes": {
      "node-red-kafkajs-broker": "src/kafka/broker.js",
      "node-red-kafkajs-producer": "src/kafka/producer.js",
      "node-red-kafkajs-consumer": "src/kafka/consumer.js"
    }
  },
  "dependencies": {
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@types/node-red": "^1.3.5",
    "node-red": "4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asinino/node-red-kafkajs"
  },
  "bugs": {
    "url": "https://github.com/asinino/node-red-kafkajs/issues"
  },
  "private": false
}
